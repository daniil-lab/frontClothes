{
    "compilerOptions": {
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": true,
      "outDir": "./build/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es6",
      "baseUrl": ".",
      "paths": {
        "Components/*": ["src/Components/*"],
        "Pages/*": ["src/Pages/*"],
        "Utils/*": ["src/Utils/*"],
      }
    },
    "include": [
      "src/index.tsx"
    ],
  }